{
    "layer": "top",
    "position": "top",
    // "margin-top": 10,
    // "margin-left": 10,
    // "margin-right": 10,
    "height": 40,
    // "width": 1650, (set for large screen)
    "modules-left": ["hyprland/workspaces", "custom/spotify"],
    "modules-center": ["custom/roblox", "custom/github", "custom/centerlogo", "custom/email", "custom/folder"],
    "modules-right": [
        "pulseaudio",   "backlight",    "network",
        "cpu",          "memory",       "battery",
        "clock"
    ],
    // modules left
    "hyprland/workspaces": {
        "format": "{id} {icon} {windows}",
        "window-rewrite-default": "Ó≠ø",
        "window-rewrite": {
            "class<kitty>": "ÔÑ° ",
            "class<vesktop>": "Û∞ôØ",
            "class<.*google.*>": "ÔÜ†",
        },
        "format-icons": {
            "active": "Û∞Ñõ",
            "default": "Ó™º",
            "visible": "Ó©∞",
        }
    },
    "custom/spotify": {
        "escape": true,
        "return-type": "json",
        "max-length": 40,
        "on-click": "playerctl play-pause",
        "smooth-scrolling-threshold": 3, // This value was tested using a trackpad, it should be lowered if using a mouse.
        "on-scroll-up" : "playerctl next",
        "on-scroll-down" : "playerctl previous",
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null | jq --unbuffered --compact-output", 
        "exec-if": "pgrep spotify"
    },
    // modules center
    "custom/roblox": {
        "format": "ùôç",
        "on-click": "dunstify 'Opening ROBLOX';flatpak run org.vinegarhq.Sober",
        "on-click-right": "dunstify 'Opening ROBLOX Studio';$HOME/scripts/roblox_studio.sh",
        "tooltip-format": "Left click: ROBLOX\nRight click: ROBLOX Studio"
    },
    "custom/github": {
        "format": "Û∞ä§",
        "on-click": "dunstify 'Opening github';xdg-open https://github.com/sebvu",
        "tooltip-format": "Github"
    },
    "custom/centerlogo": {
        "format": "¬ß",
        "tooltip-format": "sebarch"
    },
    "custom/email": {
        "format": "Û∞áÆ",
        "on-click": "dunstify 'Opening gmail';xdg-open  https://mail.google.com/mail/u/0/#inbox",
        "tooltip-format": "Gmail"
    },
    "custom/folder": {
        "format": "Û∞âã",
        "on-click": "dunstify 'Opening file manager';nemo",
        "tooltip-format": "Folder Manager"
    },
    // modules right
    "pulseaudio": {
        "format": "{volume}% {icon}",
        "format-bluetooth": "{volume}% {icon}Ôäî",
        "format-muted": "ÔÄ¶ ",
        "format-icons": {
            "headphones": "ÔÄ• ",
            "handsfree": "Û±†° ",
            "headset": "Û∞ãé ",
            "phone": "ÔÇï ",
            "portable": "ÔÇï ",
            "car": "ÔÜπ ",
            "default": ["ÔÄß ", "ÔÄ® "]
        },
        "on-click": "pavucontrol"
    },
    "backlight": {
        "format": "{percent}% {icon}",
        "states": [0,50],
        "format-icons": ["ÔÑë ", "ÔÜÖ "]
    },
    "network": {
        // "interface": "wlp2s0",
        "max-length": 30,
        "format": "{ifname}",
        "format-wifi": "{essid} ({signalStrength}%) Ôá´ ",
        "format-ethernet": "{ifname}: {ipaddr}/{cidr} Û∞äó ",
        "format-disconnected": "Disconnected ‚ö† ", //An empty format will hide the module.
        "tooltip-format": "{ifname} via {gwaddr} Û∞äó ",
        "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´ ",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ ",
        "tooltip-format-disconnected": "Disconnected",
        "on-click": "~/scripts/connect.sh"
    },
    "cpu": {
        "interval": 1,
        "max-length": 20,
        "format": "{usage}% {icon0}{icon1}{icon2}{icon3}{icon4}{icon5}{icon6}{icon7}",
        "format-icons": [
            "<span color='#69ff94'>‚ñÅ</span>", // green
            "<span color='#2aa9ff'>‚ñÇ</span>", // blue
            "<span color='#f8f8f2'>‚ñÉ</span>", // white
            "<span color='#f8f8f2'>‚ñÑ</span>", // white
            "<span color='#ffffa5'>‚ñÖ</span>", // yellow
            "<span color='#ffffa5'>‚ñÜ</span>", // yellow
            "<span color='#ff9977'>‚ñá</span>", // orange
            "<span color='#dd532e'>‚ñà</span>"  // red
        ]
    },
    "memory": {
        "interval": 30,
        "format": "{}% ÔÉâ",
        "max-length": 10,
        "tooltip-format": "{used:0.1f}G/{total:0.1f}G"
    },
    "battery": {
        "bat": "BAT0",
        "interval": 10,
        "states": {
            "warning": 30,
            "critical": 12,
        },
        "format": "{capacity}% {icon}",
        "format-icons": ["ÔâÑ ", "ÔâÉ ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "],
        "on-click": "killall wlogout;wlogout"
    },
    "clock": {
        // "timezone": "Houston/America",
        "format": "{:%H:%M} ÔÄó ",
        // "format-alt": "{:%A, %B %d, %Y (%R)}",
        "format-alt": "{:%A, %m-%d-%Y (%R)}",
        "tooltip-format": "<tt><small>{calendar}</small></tt>",
        "calendar": {
            "mode"          : "year",
            "mode-mon-col"  : 3,
            "weeks-pos"     : "right",
            "on-scroll"     : 1,
            "format": {
                "months":     "<span color='#ffead3'><b>{}</b></span>",
                "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
            }
        },
        "actions":  {
            "on-click-right": "mode",
            "on-scroll-up": "tz_up",
            "on-scroll-down": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },
}
